      - if signed_in?
        = link_to 'Sign out', destroy_user_session_path, method: :delete, class: 'uk-button uk-button-default'
      - else
        = link_to 'Sign in', new_user_session_path, class: 'uk-button uk-button-default'
        = link_to 'Sign up', new_user_registration_path, class: 'uk-button uk-button-default'
