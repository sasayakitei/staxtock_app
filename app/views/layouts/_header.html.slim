nav.uk-navbar-container.uk-margin uk-navbar="" 
  .uk-navbar-left
    = link_to 'Logo', root_path, class: 'uk-navbar-item uk-logo'
    ul.uk-navbar-nav
      li
        = link_to 'ランキング', '#'
      li
        = link_to 'カテゴリー', '#'
        / a href="#" 
        /   span.uk-icon.uk-margin-small-right uk-icon=("icon: star") 
        /   | Features
    .uk-navbar-item
      div
        | Some
        a href="#"  Link
    .uk-navbar-item
      form action="javascript:void(0)" 
        input.uk-input.uk-form-width-small placeholder="Input" type="text" /
        button.uk-button.uk-button-default Button
